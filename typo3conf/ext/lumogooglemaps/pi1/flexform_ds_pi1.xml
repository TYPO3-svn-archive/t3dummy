<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<sheets>
		<!-- Sheet General: General settings for the plugin -->
		<sGeneral>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- Width of the plugin -->
					<width>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.width</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
							</config>
						</TCEforms>
					</width>
					<!-- Height of the plugin -->
					<height>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.height</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
							</config>
						</TCEforms>
					</height>
					<!-- Type of plugin: Set from TS, map, satellite, hybrid -->
					<type>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.common.tsconfig</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type.map</numIndex>
										<numIndex index="1">map</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type.satellite</numIndex>
										<numIndex index="1">satellite</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type.hybrid</numIndex>
										<numIndex index="1">hybrid</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</type>
					<!-- Type controls: Set from TS, show, hide -->
					<type_controls>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type_controls</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.common.tsconfig</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type_controls.show</numIndex>
										<numIndex index="1">show</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.type_controls.hide</numIndex>
										<numIndex index="1">hide</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</type_controls>
					<!-- Navigation controls: Set from TS, none, small, large -->
					<nav_controls>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.nav_controls</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.common.tsconfig</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.nav_controls.none</numIndex>
										<numIndex index="1">none</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.nav_controls.small</numIndex>
										<numIndex index="1">small</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.nav_controls.large</numIndex>
										<numIndex index="1">large</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</nav_controls>
					<!-- Default zoom level: Auto, set from TS, 1 - 18 -->
					<zoom>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.auto</numIndex>
										<numIndex index="1">auto</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.common.tsconfig</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="6" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.5</numIndex>
										<numIndex index="1">5</numIndex>
									</numIndex>
									<numIndex index="7" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.6</numIndex>
										<numIndex index="1">6</numIndex>
									</numIndex>
									<numIndex index="8" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.7</numIndex>
										<numIndex index="1">7</numIndex>
									</numIndex>
									<numIndex index="9" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.8</numIndex>
										<numIndex index="1">8</numIndex>
									</numIndex>
									<numIndex index="10" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.9</numIndex>
										<numIndex index="1">9</numIndex>
									</numIndex>
									<numIndex index="11" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.10</numIndex>
										<numIndex index="1">10</numIndex>
									</numIndex>
									<numIndex index="12" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.11</numIndex>
										<numIndex index="1">11</numIndex>
									</numIndex>
									<numIndex index="13" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.12</numIndex>
										<numIndex index="1">12</numIndex>
									</numIndex>
									<numIndex index="14" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.13</numIndex>
										<numIndex index="1">13</numIndex>
									</numIndex>
									<numIndex index="15" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.14</numIndex>
										<numIndex index="1">14</numIndex>
									</numIndex>
									<numIndex index="16" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.15</numIndex>
										<numIndex index="1">15</numIndex>
									</numIndex>
									<numIndex index="17" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.16</numIndex>
										<numIndex index="1">16</numIndex>
									</numIndex>
									<numIndex index="18" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.17</numIndex>
										<numIndex index="1">17</numIndex>
									</numIndex>
									<numIndex index="19" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.general.zoom.18</numIndex>
										<numIndex index="1">18</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</zoom>
				</el>
			</ROOT>
		</sGeneral>
		<!-- Sheet General: End -->
		<!-- Sheet Data: Data to be displayed -->
		<sData>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- Longitude of center point -->
					<center_longitude>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.center.longitude</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>trim,nospace</eval>
							</config>
						</TCEforms>
					</center_longitude>
					<!-- Latitude of center point -->
					<center_latitude>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.center.latitude</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>trim,nospace</eval>
							</config>
						</TCEforms>
					</center_latitude>
					<!-- Control when center point will be shown -->
					<center_show>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.center.show</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.common.tsconfig</numIndex>
										<numIndex index="1">ts</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.center.show.ifempty</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.center.show.always</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</center_show>
					<!-- Startingpoint for data -->
					<startingpoint>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.startingpoint</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>10</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</startingpoint>
					<!-- Recursion level for data search -->
					<recursive>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.0</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.1</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.2</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.3</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.4</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.data.recursive.infinite</numIndex>
										<numIndex index="1">255</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</recursive>
				</el>
			</ROOT>
		</sData>
		<!-- Sheet Data: End -->
		<!-- Sheet Template: Templates for map, sidebar, infobox, marker icons -->
		<sTemplate>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- Template file -->
					<template_file>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.template_file</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>tmpl,html</allowed>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_lumogooglemaps/</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</template_file>
					<!-- Marker icon -->
					<marker_icon>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_icon</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_lumogooglemaps/</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</marker_icon>
					<!-- Marker shadow -->
					<marker_shadow>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_shadow</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>500</max_size>
								<uploadfolder>uploads/tx_lumogooglemaps/</uploadfolder>
								<maxitems>1</maxitems>
								<size>1</size>
								<selectedListStyle>width:180px</selectedListStyle>
							</config>
						</TCEforms>
					</marker_shadow>
					<!-- X coord in image for spot -->
					<marker_spot_x>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_spot_x</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</marker_spot_x>
					<!-- Y coord in image for spot -->
					<marker_spot_y>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_spot_y</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</marker_spot_y>
					<!-- X coord in image for info box -->
					<marker_info_x>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_info_x</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</marker_info_x>
					<!-- Y coord in image for info box -->
					<marker_info_y>
						<TCEforms>
							<label>LLL:EXT:lumogooglemaps/pi1/locallang.xml:flexform.sheets.template.marker_info_y</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</marker_info_y>
				</el>
			</ROOT>
		</sTemplate>
		<!-- Sheet Template: End -->
	</sheets>
</T3DataStructure>
